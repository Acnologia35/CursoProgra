function Randomizador(){
    var NrandomE = Math.floor(Math.random() * 10 + 1)
console.log(NrandomE);}

var ArrayVacio = new Array(30);
let longitud = ArrayVacio.length

for(posicion = 0; posicion < longitud; posicion++ ){;
    ArrayVacio.push(Randomizador());
    console.log(ArrayVacio[posicion]);}


